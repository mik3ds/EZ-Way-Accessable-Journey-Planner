<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.user.testnav2">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.READ_PHONE_STATE" />
    <uses-permission android:name="android.permission.FOREGROUND_SERVICE" />
    <uses-permission android:name="android.permission.VIBRATE" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ezway"
        android:label="@string/app_name"
        android:networkSecurityConfig="@xml/network_security_config"
        android:roundIcon="@drawable/ezway"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:usesCleartextTraffic="true"
        tools:replace="android:icon">
        <activity android:name=".WelcomeScreenActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <service
            android:name="com.amazonaws.mobileconnectors.s3.transferutility.TransferService"
            android:enabled="true" />

        <activity android:name=".Tutorial1" />
        <activity android:name=".Tutorial2" />
        <activity android:name=".Tutorial3" />
        <activity android:name=".Tutorial4" />
        <activity android:name=".Tutorial5" />
        <activity android:name=".Tutorial6" />
        <activity android:name=".ProfileActivity" />
        <activity android:name=".ProfileEditActivity" />
        <activity android:name=".TrackingParentEditActivity" />
        <activity
            android:name=".MapActivity"
            android:label="@string/title_activity_main3"
            android:theme="@style/AppTheme" />
        <activity android:name=".TrackingChildActivity" />
        <activity android:name=".TrackingChildEditActivity" />
        <activity android:name=".TrackingParentActivity" />
        <activity android:name=".StepByStepActivity" />
        <activity android:name=".stepByStep3" />
        <activity android:name=".StepByStep2Activity"/>
    </application>

</manifest>